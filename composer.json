{
  "name": "kriss/multi-process",
  "description": "Multi Async Process Manager based on symfony/process",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "kriss",
      "email": "462679766@qq.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "laravel/serializable-closure": "^1.0",
    "symfony/console": "^5|^6",
    "symfony/process": "^5|^6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
